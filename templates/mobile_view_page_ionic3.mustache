{{=<% %>=}}
<ion-item>
    <core-tabs>
        <%#tabList%>
            <core-tab [title]="'<%title%>'">
                <ng-template>
                    <%#html%>
                        <ion-item class="ion-text-wrap">
                            <core-format-text text="'<% html %>'"></core-format-text>
                        </ion-item>>
                    <%/html%>
                    <%^html%>
                     <core-iframe [src]="'<% url %>'" [allowFullscreen]="1" [iframeHeight]="1000"></core-iframe>
                    <%/html%>
                </ng-template>
            </core-tab>
        <%/tabList%>
    </core-tabs>
</ion-item>
